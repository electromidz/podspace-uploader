{
  "name": "@total-typescript/podspace-uploader",
  "version": "0.0.1",
  "description": "This package use for uploading file on PodSpace",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
