  "bugs": {
    "url": "https://github.com/electromidz/podspace-uploader/issues"
  },
  "homepage": "https://github.com/electromidz/podspace-uploader#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  }
}
